{
  "name": "@maiertech/eslint-config",
  "version": "0.8.0",
  "author": "Thilo Maier <git@maier.asia>",
  "description": "Sharable ESLint config for @maiertech projects.",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "repository": "github:maiertech/eslint-config",
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "lint": "eslint . --ignore-path .gitignore",
    "fix": "eslint . --ignore-path .gitignore --fix",
    "format": "pretty-quick"
  },
  "peerDependencies": {
    "babel-eslint": "9.x",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x"
  },
  "devDependencies": {
    "@changesets/cli": "^2.6.5",
    "babel-eslint": "9.x",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "husky": "^1.1.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.6.0"
  }
}
