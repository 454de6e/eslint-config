{
  "name": "@mdotasia/eslint-config",
  "version": "0.6.0",
  "author": "Thilo Maier <git@maier.asia>",
  "description": "Sharable ESLint config for @mdotasia projects.",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "repository": "mdotasia/eslint-config",
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "upgrade-interactive": "npm-check --update"
  },
  "dependencies": {
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.11.1"
  },
  "peerDependencies": {
    "eslint": "^5.15.1"
  },
  "devDependencies": {
    "eslint": "^5.15.1",
    "husky": "^1.1.0",
    "lint-staged": "^8.1.5",
    "npm-check": "^5.9.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.6.0"
  }
}
